    "force-build": "./bin/run.sh build --force",
    "test": "./bin/run.sh test",
    "lint": "./bin/run.sh test",
    "serve": "./bin/run.sh serve"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.0",
    "worker-loader": "^3.0.8"
